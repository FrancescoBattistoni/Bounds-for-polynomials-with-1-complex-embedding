  /* z = 1: (1+x)*(1+2*x*g+x^2)*(1-x)*2*(1-g)*2*(1+g)*(1-2*22/25*g+(22/25)^2)*2*sqrt(1-g^2) */
  L= (1+x)*(1+2*x*g+x^2)*(1-x)*2*(1-g)*2*(1+g)*(1-2*22/25*g+(22/25)^2) /* times 2*sqrt(1-g^2) */
  Tx=factor(deriv(L,x))
  Tg=factor(deriv(L,g)*(1-g^2)-g*L)
  Lx=Tx[4,1]
  Lg=Tg[5,1]
  
  T1=factor(polresultant(Lx,Lg,g)) 
  res1g=T1[3,1]
  
  R(x,g)=(1+x)*(1+2*x*g+x^2)*(1-x)*2*(1-g)*2*(1+g)*(1-2*22/25*g+(22/25)^2)*2*sqrt(1-g^2);
  
  search(thresold)={
    tot=0;
    VX=polrootsreal(res1g,[0,1]);
    if (#VX > 0,
       
        print(#VX);
        for(ix=1,#VX,
           VG = polrootsreal(subst(Lx,x,VX[ix]),[0,1]);
           if (#VG > 0,
      
              for(ig=1,#VG,
                  tot+=1;
                  value=R(VX[ix],VG[ig]);
                  if(value>thresold,
                      print("[x,g]=",[VX[ix],VG[ig]]);
                      print("R(x,g)=",value)
                    )  
                  );
               );
            );
         );   
  print("number of stationary points: ",tot)
  };
  
  search(9);
