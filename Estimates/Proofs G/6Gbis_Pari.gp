  /* g = 0: (1+x)*(1+x^2)*(1-x*z)*2*(1+z^2)*(1+(z*22/25)^2)*2 */
  L= (1+x)*(1+x^2)*(1-x*z)*2*(1+z^2)*(1+(z*22/25)^2)*2
  Tx=factor(deriv(L,x))
  Tz=factor(deriv(L,z))
  Lx=Tx[3,1]
  Lz=Tz[2,1]
  
  T1=factor(polresultant(Lx,Lz,z)) 
  res1z=T1[2,1]
  
  R(x,z)=(1+x)*(1+x^2)*(1-x*z)*2*(1+z^2)*(1+(z*22/25)^2)*2;
  
  search(thresold)={
    tot=0;
    VX=polrootsreal(res1z,[0,1]);
    if (#VX > 0,
       
        print(#VX);
        for(ix=1,#VX,
           VZ = polrootsreal(subst(Lx,x,VX[ix]),[3/5,1]);
           if (#VZ > 0,
      
              for(iz=1,#VZ,
                  tot+=1;
                  value=R(VX[ix],VZ[iz]);
                  if(value>thresold,
                      print("[x,z]=",[VX[ix],VZ[iz]]);
                      print("R(x,z)=",value)
                    )  
                  );
               );
            );
         );   
  print("number of stationary points: ",tot)
  };
  
